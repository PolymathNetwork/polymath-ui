{
  "name": "@polymathnetwork/ui",
  "version": "1.0.12",
  "homepage": "https://github.com/PolymathNetwork/polymath-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/PolymathNetwork/polymath-ui.git"
  },
  "bugs": {
    "url": "https://github.com/PolymathNetwork/polymath-ui/issues"
  },
  "main": "build/dist/index.js",
  "typings": "build/dist/index.d.ts",
  "scripts": {
    "start": "docz dev",
    "copy-images": "copyfiles -u 1 src/images/*.{png,svg} src/images/icons/*.svg build/dist/",
    "build:website": "docz build",
    "test": "jest --env=jsdom",
    "build": "ttsc --skipLibCheck && yarn copy-images",
    "preinstall": "rm -rf node_modules/websocket/.git",
    "semantic-release": "semantic-release"
  },
  "resolutions": {
    "react-hot-loader": "4.7.1",
    "@types/react": "16.8.3",
    "@types/react-dom": "16.8.2",
    "docz-core": "^0.13.7",
    "ansi-styles": "^3.2.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "@polymathnetwork/sdk": "^2.0.0",
    "@sentry/browser": "^4.4.1",
    "@types/file-saver": "^2.0.1",
    "@types/json2csv": "^4.3.3",
    "@types/luxon": "^1.15.2",
    "@types/numeral": "0.0.25",
    "@types/papaparse": "^5.0.1",
    "@types/react-redux": "^7.1.1",
    "@types/redux": "^3.6.0",
    "@types/styled-system": "^5.0.0",
    "@types/web3": "^1.0.19",
    "bignumber.js": "8.0.1",
    "commitizen": "^4.0.3",
    "file-saver": "^2.0.2",
    "flatpickr": "^4.5.2",
    "formik": "^1.5.1",
    "json2csv": "^4.2.0",
    "lodash": "4.17.19",
    "luxon": "^1.17.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "numeral": "^2.0.6",
    "papaparse": "^5.0.0",
    "polished": "^2.3.1",
    "popper.js": "^1.14.7",
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "react-dropzone": "^9.0.0",
    "react-modal": "^3.6.1",
    "react-router-dom": "^4.3.1",
    "react-select": "2.4.1",
    "react-table": "7.0.0-alpha.0",
    "react-tooltip": "^3.9.0",
    "redux": "3.x",
    "redux-little-router": "^15.1.2",
    "scrollbarwidth": "^0.1.3",
    "semantic-release-cli": "^5.1.1",
    "simplebar-react": "^0.1.4",
    "styled-components": "4.1.3",
    "styled-system": "^3.2.1",
    "tooltip.js": "^1.3.1",
    "tsconfig-paths": "^3.8.0",
    "typeface-overpass": "^0.0.54",
    "web3": "^1.0.0-beta.37",
    "yup": "^0.26.10"
  },
  "devDependencies": {
    "@babel/core": "7.1.0",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-proposal-export-default-from": "7.0.0",
    "@babel/plugin-syntax-async-generators": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.0.0",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@emotion/cache": "^10.0.0",
    "@emotion/core": "^10.0.6",
    "@semantic-release/git": "^7.1.0-beta.3",
    "@svgr/webpack": "^4.1.0",
    "@types/jest": "^23.3.10",
    "@types/lodash": "^4.14.119",
    "@types/moment-timezone": "^0.5.10",
    "@types/node": "^10.12.12",
    "@types/react": "16.8.3",
    "@types/react-dom": "^16.8.2",
    "@types/react-modal": "^3.8.0",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-select": "2.0.11",
    "@types/react-tooltip": "^3.9.0",
    "@types/redux-little-router": "^15.1.0",
    "@types/styled-components": "4.1.4",
    "@types/yup": "^0.26.10",
    "@zerollup/ts-transform-paths": "^1.7.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "8.0.2",
    "babel-plugin-named-asset-import": "^0.3.0",
    "copyfiles": "^2.1.1",
    "cz-conventional-changelog": "^3.0.2",
    "docz": "^0.13.7",
    "docz-plugin-css": "^0.11.0",
    "docz-theme-default": "^0.13.7",
    "emotion-theming": "^10.0.6",
    "husky": "^3.0.1",
    "jest": "^24.1.0",
    "jest-dom": "^3.1.1",
    "jest-styled-components": "^6.3.1",
    "namor": "^1.1.1",
    "react-hot-loader": "^4.7.1",
    "react-redux": "4.x",
    "react-testing-library": "^5.8.0",
    "redux-little-router": "^15.1.2",
    "semantic-release": "16.0.0-beta.18",
    "ts-node": "^8.3.0",
    "ttypescript": "^1.5.7",
    "typescript": "3.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.(ts|tsx|js)"
    ],
    "testURL": "http://localhost",
    "testPathIgnorePatterns": [
      "build"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/../../config/babelJestWrapper",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/fileTransform.js"
    },
    "moduleNameMapper": {
      "~/(.*)": "<rootDir>/src/$1",
      "\\.(css|scss)$": "<rootDir>/config/cssTransform.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/testUtils/setupTests.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,mjs}"
    ],
    "coverageReporters": [
      "lcov",
      "cobertura",
      "text"
    ],
    "coverageDirectory": "<rootDir>/reports",
    "reporters": [
      "jest-junit",
      "default"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
